{
    "family": "meu-app-task",
    "taskRoleArn": "arn:aws:iam::212199569343:role/action_ecs",
    "executionRoleArn": "arn:aws:iam::212199569343:role/action_ecs",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "fastapi-container",
        "image": "212199569343.dkr.ecr.us-east-1.amazonaws.com/repo:latest",
        "cpu": 256,
        "memory": 512,
        "memoryReservation": 256,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "restartPolicy": {
          "enabled": true,
          "name": "on-failure"
        },
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/meu-app-logs",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "meu-container"
          }
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "ephemeralStorage": {
      "sizeInGiB": 20
    }
}